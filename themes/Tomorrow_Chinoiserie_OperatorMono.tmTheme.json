{
  "name": "Tomorrow - Chinoiserie",
  "tokenColors": [
    {
      "settings": {
        "background": "#FFFFFF",
        "caret": "#4A4035",
        "foreground": "#4A4035",
        "invisibles": "#80766E",
        "lineHighlight": "#80766E",
        "selection": "#D6D6D6"
      }
    },
    {
      "name": "Italics",
      "scope": [
        "emphasis",
        "storage",
        "comment",
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.tag.jade",
        "entity.other.attribute-name.tag.pug",
        "markup.italic",
        "keyword.control",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "strong",
        "markup.bold",
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#80766E"
      }
    },
    {
      "name": "Foreground",
      "scope": [
        "keyword.operator.class",
        "constant.other",
        "source.php.embedded.line"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#4A4035"
      }
    },
    {
      "name": "Variable, String Link, Regular Expression, Tag Name",
      "scope": [
        "variable",
        "support.other.variable",
        "string.other.link",
        "string.regexp",
        "entity.name.tag",
        "entity.other.attribute-name",
        "meta.tag",
        "declaration.tag"
      ],
      "settings": {
        "foreground": "#C04851"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "variable.parameter",
        "punctuation.section.embedded",
        "keyword.other.unit"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FB8B05"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#D6A01D"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "entity.other.inherited-class",
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#428675"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#2B73AF"
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method",
        "meta.block-level"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#815C94"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "entity.name.tag.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#C08EAF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#E0E0E0",
        "fontStyle": "",
        "foreground": "#2F2F35"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "background": "#815C94",
        "foreground": "#2F2F35"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#945833",
        "fontStyle": "",
        "foreground": "#2F2F35"
      }
    }
  ]
}
